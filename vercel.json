{
    "version": 2,
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/public/$1"
        }
    ],
    "functions": {
        "api/**/*.js": {
            "runtime": "nodejs20.x"
        }
    }
}